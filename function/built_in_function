ğŸŒšceil:
double x = 8.0 / 7.0;     
cout << x << endl;          //x=1.14286
cout << ceil(x);            //x=2
cout << ceil(8,7);          //x=2

ğŸŒšfloor:
double x = 9.89;
cout << floor(x) <<endl     //9

ğŸŒšround:
double x = 9.89;
cout << round(x) <<endl      //10
double y = 9.42;
cout << round(y) <<endl      //9

ğŸŒšreverse:
int ara[5] = {1,2,3,4,5}
reverse(&ara[0], &ara[3]); or, reverse(ara,ara+3);
for(int i=0; i<5; i++)}
cout<<ara[i]<<" ";
cout<<endl;            //3 2 1 4 5
}

ğŸŒšsort:
int ara[7] = {6,1,3,2,100,7,159};
sort(ara+2,ara+6);
for(int i=0; i<7; i++){
   cout<<ara[i]<<" ";          //6 1 2 3 7 100 150
   cout<<endl;
 }

ğŸŒšmax_element:
int ara[5] = {3,1,2,5,4};
cout << *max_element(ara, ara+5) << endl;      //5

ğŸŒšmin_element:
int ara[5] = {3,1,2,5,4};
cout << *min_element(ara, ara+5) << endl;      //1


ğŸŒš__gcd(greatest common divider):
cout << __gcd(24,16) << endl;           //8


ğŸŒš__lcm:
int lcm(int a, int b) {
      return (a * b) / __gcd(a,b);
    }
int main(){
    int n;
   cin >> n;
   cout << lcm(16, 24) << endl;
   return 0;
   }


ğŸŒšisupper and isupper:
char c = 'A';
cout << islower(c) << endl;          //a

char s = 'z';
cout << isupper(s) << endl;           //Z


ğŸŒšsqrt():
double x = 25;
cout << sqrt(x) << endl;        //5


ğŸŒšcbrt():
double x = 27;
cout << cbrt(x) << endl;         //3


ğŸŒštoupper and tolower:
char c = 'b';
cout << (char)toupper(c) << endl;            //B
cout << (char)tolower(c) << endl;           //b


ğŸŒšstrcpy:
char str1[30] = "hello world", str2[7] = "myworld";
strcpy(str1 + 6, str2);
cout << str1 << endl;               //hello myworld








